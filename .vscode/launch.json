{
    "version": "2.0.0",
    "configurations": [
        {
            "type": "msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://127.0.0.1:5174/",
            "webRoot": "${workspaceFolder}"
        }
    ],
    "tasks": [
        {
            "label": "Run npm start",
            "type": "shell",
            "command": "npm run start",
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Run GitHub Actions Runner",
            "type": "shell",
            "command": "C:\\actions-runner\\run.cmd",
            "problemMatcher": [],
            "group": "none"
        }
    ]
}